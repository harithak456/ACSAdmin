    @*tap*@


    <script>
        $(document).ready(function () {

            $('.subCategory').click(function (e) {
                var path = window.location.pathname;
                if (path == "/") {
                    var subCategoryId = $(this).attr('id');
                    window.location.href = "../Home/ProductsList?categoryId=0&SubCategory=" + subCategoryId + "";
                    return false;
                }
                else {
                    e.preventDefault();
                    var subCategoryId = $(this).attr('id');
                    $.ajax({
                        type: 'POST',
                        url: '/Home/NavPartialProduct',
                        cache: false,
                        data: { categoryId: 0, subCategoryId: subCategoryId },
                        dataType: 'html',
                        success: function (data) {
                            $('.categories_title').trigger('click');
                            $('#content').html(data)

                        }
                    });
                }
            });

            $('#btnSearch').click(function (e) {
                e.preventDefault();
                var SearchText = $('#SearchText').val().trim();
                $.ajax({
                    type: 'POST',
                    url: '/Home/SearchProducts',
                    cache: false,
                    data: { SearchText: SearchText },
                    dataType: 'html',
                    success: function (data) {
                        $('#content').html(data)
                    }
                });
            });

            $('.btnRemoveItem').click(function (e) {
                e.preventDefault();
                var CartID = $(this).attr('id');
                $.ajax({
                    type: 'POST',
                    url: '/Order/DeleteCart',
                    data: "{ 'CartID': '" + CartID + "' }",
                    contentType: "application/json; charset=utf-8",
                    dataType: 'json',
                    success: function (data) {
                        var div = document.getElementById(CartID);
                        div.remove();
                        $('.cart_quantity').html(data);
                    }
                });
            });
        });
    </script>
