    <script>
        $(document).ready(function () {


            //Add to cart
            $('#AddToCart').click(function () {
                
                var CategoryId = $('#categoryid').val();
                var productId = $('#productid').val();
                $.ajax({
                    type: 'POST',
                    url: '/Order/AddToCart',
                    data: "{ 'Product_ID': '" + productId + "' }",
                    contentType: "application/json; charset=utf-8",
                    dataType: 'json',
                    success: function () {
                        var subCategoryId = "";
                        var brands = "";
                        var arrayCategory = [];
                        $('input:checked.subCategoryFilter').each(function () {
                            arrayCategory.push($(this).val());
                            if (subCategoryId == "")
                                subCategoryId = $(this).val();
                            else
                                subCategoryId = subCategoryId + "," + $(this).val();
                        })

                        var arrayBrand = [];
                        $('input:checked.brandFilter').each(function () {
                            arrayBrand.push($(this).val());
                            if (brands == "")
                                brands = $(this).val();
                            else
                                brands = brands + "," + $(this).val();
                        })
                        window.location.href = "../Home/ProductsList?categoryId=" + CategoryId + "&SubCategory=" + subCategoryId + "&brand=" + brands + "";
                    },
                    error: function () {
                        console.log('failed');
                    }
                });
            });

            $('.subCategory').click(function (e) {
                var path = window.location.pathname;
                if (path == "/") {
                    var subCategoryId = $(this).attr('id');
                    window.location.href = "../Home/ProductsList?categoryId=0&SubCategory=" + subCategoryId + "";
                    return false;
                }
                else {
                    e.preventDefault();
                    var subCategoryId = $(this).attr('id');
                    $.ajax({
                        type: 'POST',
                        url: '/Home/NavPartialProduct',
                        cache: false,
                        data: { categoryId: 0, subCategoryId: subCategoryId },
                        dataType: 'html',
                        success: function (data) {
                            $('.categories_title').trigger('click');
                            $('#content').html(data)

                        }
                    });
                }
            });

            $('#SearchText').bind("enterKey", function (e) {
             
                e.preventDefault();
                var SearchText = $('#SearchText').val().trim(); 
                $.ajax({
                    type: 'POST',
                    url: '/Home/SearchProducts',
                    cache: false,
                    data: { SearchText: SearchText },
                    dataType: 'html',
                    success: function (data) {
                        $('#content').html(data)
                    }
                });
            });
            $('#SearchText').keyup(function (e) {                  
                if (e.keyCode == 13) {
                    $(this).trigger("enterKey");
                }
            });


            $('#btnSearch').click(function (e) {               
                e.preventDefault();
                var SearchText = $('#SearchText').val().trim();
                $.ajax({
                    type: 'POST',
                    url: '/Home/SearchProducts',
                    cache: false,
                    data: { SearchText: SearchText },
                    dataType: 'html',
                    success: function (data) {
                        $('#content').html(data)
                    }
                });
            });

            $('.btnRemoveItem').click(function (e) {
              
                e.preventDefault();
                var CartID = $(this).attr('id');
                $.ajax({
                    type: 'POST',
                    url: '/Order/DeleteCart',
                    data: "{ 'CartID': '" + CartID + "' }",
                    contentType: "application/json; charset=utf-8",
                    dataType: 'json',
                    async:false,
                    success: function (data) {
                        var div = document.getElementById(CartID);
                        div.remove();                      
                        $('.cart_quantity').html(data[0]);
                        $('.cartsubtotal').html("AED "+ data[2]);
                        $('.carttotal').html("AED "+ data[1]);                     
                    }
                });
            });
        });
    </script>
